<div class="ui modal">
  <div class="header">List of 10 Students, click the arrow keys to get more students</div>
  <div class="content">
    {{#each residencyModel as |oneResidency|}}
      <div class="ui grid">
        <div class="ten wide column">
          <div class="actions">
            <div class="ui form">
              <div class="grouped fields">
                <div class="field">
                  <div class="ui radio checkbox">
                    <input type="radio" name = 'residency'
                           <!--onchange={{action 'getResidency' oneResidency}}>-->
                    <label>{{oneResidency.name}}</label>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="six wide column">
        </div>
      </div>
    {{/each}}
  </div>
</div>

