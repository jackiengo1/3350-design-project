<div class="ui center aligned header">
  <h2>Add Residency</h2>
</div>

{{#each residencyModel as |oneResidency|}}
<div class="ui radio checkbox">
  <input type="radio" name ="residency" onchange={{action 'getResidence' oneResidency}}>
  <label>{{oneResidency.name}}</label>
</div>
{{/each}}
<button class="ui button" onclick={{action 'deleteResidency'}}>Delete</button>

<div class="ui grid">
  <div class="ui left aligned  seven wide column">
    <div class="ui form">
      <div class="inline field">
        <label>Residency</label>
        {{input type="text"  value = residencyName}}
      </div>
    </div>
  </div>
</div>

<div class="ui center aligned header">
  <button class="ui primary button" {{action 'addResidency'}}>
    Add Residency
  </button>
</div>
